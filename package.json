{
  "name": "chain-db-cli",
  "version": "1.0.0",
  "description": "CLI para interagir com o ChainDB",
  "main": "dist/index.js",
  "bin": {
    "chaindb": "./dist/index.js"
  },
  "engines": {
    "node": ">=20.18.3"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "start": "node --no-deprecation dist/index.js",
    "dev": "ts-node --no-deprecation src/index.ts",
    "prepublishOnly": "npm run build",
    "link": "npm run build && npm unlink -g chain-db-cli && npm link"
  },
  "keywords": [
    "chaindb",
    "cli",
    "database"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^8.2.6",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.12",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}